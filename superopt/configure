#echo "1;" >> $config_host_pm;

cd "$build_dir/etfg_i386" && cmake -G "Ninja" -D CMAKE_C_COMPILER=clang-11 -D CMAKE_CXX_COMPILER=clang++-11 -DCMAKE_BUILD_TYPE=etfg_i386 ../..
